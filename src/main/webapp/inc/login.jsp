<meta charset="UTF-8">

<div class="modal fade" id="exampleModal" tabindex="-1"
	aria-labelledby="exampleModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="exampleModalLabel">Welcome to
					Tourism in Middle Earth</h5>
				<button type="button" class="btn-close" data-bs-dismiss="modal"
					aria-label="Close"></button>
			</div>
			<div class="modal-body">
				<div class="mb-3">
					<label for="exampleInputEmail1" class="form-label" name="username">Username</label>
					<input type="email" class="form-control" id="exampleInputEmail1"
						aria-describedby="emailHelp">
					<div class="mb-3">
						<label for="exampleInputPassword1" class="form-label" name="password">Password</label>
						<input type="password" class="form-control"
							id="exampleInputPassword1">
					</div>
					<div id="emailHelp" class="form-text">Never share your
						credentials with anyone else.</div>
				</div>

			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary"
					data-bs-dismiss="modal">Cancel</button>
				<button type="submit" class="btn btn-primary">Submit</button>
			</div>
		</div>
	</div>
</div>